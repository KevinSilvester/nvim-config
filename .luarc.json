{
   "$schema": "https://raw.githubusercontent.com/sumneko/vscode-lua/master/setting/schema.json",
   "runtime": {
      "version": "LuaJIT"
   },
   "workspace": {
      "library": ["lua", "$VIMRUNTIME", "${3rd}/luv/library"],
      "checkThirdParty": false
   },
   "diagnostics": {
      "globals": ["vim"],
      "unusedLocalsExclude": ["_*"]
   }
}
